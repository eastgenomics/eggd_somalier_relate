{
  "name": "eggd_somalier_relate",
  "title": "Somalier relate",
  "summary": "Somalier relate",
  "dxapi": "1.0.0", 
  "properties": {
    "githubRelease": "v1.0.0"
  },
  "inputSpec": [
    {
      "name": "input_file",
      "class": "array:file",
      "optional": false,
      "patterns": [
        "*.somalier"
      ],
      "help": "An array of somalier files"
    },
    {
      "name": "f",
      "label": "Female",
      "class": "string",
      "optional": true,
      "help": "A integer value for het calls threshold for females. Default is 45"
    },
    {
      "name": "m",
      "label": "Male",
      "class": "string",
      "optional": true,
      "help": "A integer value for het calls threshold for males. Default is 1"
    }
  ],
  "outputSpec": [
    {
      "name": "html",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "pairs_tsv",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "samples_tsv",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "groups_tsv",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    },
    {
      "name": "ped_output",
      "class": "file",
      "patterns": [
        "*"
      ],
      "help": ""
    }
  ],
  "runSpec": {
    "timeoutPolicy": {
      "*": {
        "hours": 2
      }
    },
    "interpreter": "bash",
    "release": "16.04",
    "distribution": "Ubuntu",
    "file": "src/eggd_somalier_relate.sh",
    "version": "1"
  },
  "access": {
    "project": "CONTRIBUTE",
    "allProjects": "VIEW",
    "network": [
      "*"
    ]
  },
  "regionalOptions": {
    "aws:eu-central-1": {
      "systemRequirements": {
        "*": {
          "instanceType": "mem1_ssd1_v2_x4"
        }
      }
    }
  }
}
